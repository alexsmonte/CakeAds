{
    "name": "asmpkg/CakeAds",
    "description": "CakeAds plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0",
        "josegonzalez/cakephp-upload": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeAds\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeAds\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
